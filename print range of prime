#include <stdio.h>
int prime(int);
int main()
{
    int n1,n2,j,temp=0;
    printf("enter the range\n");
    scanf("%d%d",&n1,&n2);
    if(n1<n2)
    for(j=n1;j<=n2;j++)
    {
    prime(n1);
    if(temp==0)
    printf("%d ",n1);
    }
    else
    for(j=n1;j>=n2;j--)
    {
    prime(n1);
    if(temp==0)
    printf("%d ",n1);
    }
}
int prime(n1)
{
    int i,num,temp=0;
    for(i=2;i!=num;i++)
    if(num%i==0)
    temp=1;
}
